{
  "name": "cryptolizers",
  "version": "1.0.7",
  "description": "Audio visualizer library for the cryptolizers project.",
  "main": "dist/bundle.cjs.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npx npm-run-all --parallel server watch",
